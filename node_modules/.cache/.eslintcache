 => {\r\n            return item.id !== id\r\n        \r\n        });\r\n        setCart(cartFilter);\r\n    }\r\n\r\n    const agregaItems = (id) => {\r\n        const newCart = [...cart]\r\n        const cartAgrega = newCart.map(item => {\r\n            if( item.id == id){\r\n                item.stock == item.cant ? item.cant = item.stock : item.cant += 1\r\n            }\r\n        \r\n        });\r\n        setCart(newCart);\r\n    }\r\n\r\n    const restaItems = (id) => {\r\n        const newCart = [...cart]\r\n        const cartAgrega = newCart.map(item => {\r\n            if( item.id == id){\r\n                item.cant <= 1 ? item.cant = 1 : item.cant = item.cant - 1\r\n            } \r\n        \r\n        });\r\n        setCart(newCart);\r\n    }\r\n\r\n\r\n    const clearCart = () => {\r\n        setCart([]);\r\n     }\r\n    \r\n\r\n    const isInCart = (id) => {\r\n        if(cart.some(item => item === id)){ \r\n            return true \r\n        }else{\r\n            return false;\r\n        }\r\n }\r\n\r\n const cantInCart = () => {\r\n    let cant = 0;\r\n    cart.forEach(element => cant += element.cant)\r\n    return cant;\r\n }\r\n\r\nconst calcPriceCart = () => {\r\n    let total = 0;\r\n    cart.forEach(element => total += element.precio * element.cant);\r\n    return total; \r\n}\r\n\r\nconst contextFunction = () => console.log('');\r\n    return(\r\n        <Provider value={{contextFunction,\r\n                            cart, \r\n                            addtoCart, \r\n                            removeFromCart, \r\n                            clearCart, \r\n                            cantInCart,\r\n                            restaItems,\r\n                            agregaItems, \r\n                            calcPriceCart}}>\r\n            {children}\r\n        </Provider>\r\n    )\r\n\r\n}\r\n\r\nexport default useCartContext;","C:\\Users\\Cacti\\Desktop\\REACT\\Macrofarma\\macrofarma2\\src\\components\\Shop\\ShopContainer.js",[],